<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" CanResize="False" Width="400" Height="250"
        x:Class="Astrare.About" WindowStartupLocation="CenterScreen"
        Title="About" Icon="astrareIcon.png" Background="#121010">
    <StackPanel>
        <Image Source="astrareLogo.png" Height="75" HorizontalAlignment="Center"/>
        <Label HorizontalAlignment="Center">
            <TextBlock TextAlignment="Center" TextWrapping="Wrap">
                Astrare is an astronomical ephemerides generation software made by Raouf Ould Ali (github.com/SpeedCode210)
            </TextBlock>
        </Label>
        <Label HorizontalAlignment="Center">
            <TextBlock TextAlignment="Center" TextWrapping="Wrap">
                Using KosmorroLib Python library made by Jérôme Deuchnord (github.com/Deuchnord)
            </TextBlock>
        </Label>
        <Label HorizontalAlignment="Center">
            <TextBlock Name="ExecutingVersion" TextAlignment="Center" TextWrapping="Wrap"/>
        </Label>
    </StackPanel>
</Window>
